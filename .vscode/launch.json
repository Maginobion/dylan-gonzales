  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "[NG] Launch Chrome debug",
      "url": "http://127.0.0.1:4204",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}/pillars-ui"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "[Next] Launch Chrome debug",
      "url": "http://127.0.0.1:3001",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}/pillars-ui-v2"
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch Edge debug",
      "url": "http://127.0.0.1:4204",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}/pillars-ui"
    },
    {
      "name": "ATTACH TO NEXT",
      "type": "node",
      "request": "attach",
      "port": 9231,
      "sourceMaps": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/pillars-ui-v2"
    },
    {
      "name": "ATTACH TO API",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "restart": true,
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/pillars-api/dist/**/*"
      ]
    },
    {
      "name": "ATTACH TO FINANCIAL CONSUMER",
      "type": "node",
      "request": "attach",
      "port": 20238,
      "restart": true,
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/pillars-api/dist/**/*"
      ]
    },
    {
      "name": "---- ADMIN CRONJOBS -----",
      "type": "node",
      "request": "attach",
      "port": 99999,
      "restart": true,
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/pillars-api/dist/**/*"
      ]
    },
    {
      "name": "DAILY USER GROWTH REPORT",
